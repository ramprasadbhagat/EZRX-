            {
              "id": "59362120BB87461E882994CE85178C27",
              "name": "Reaching us is easy",
              "template": {
                "name": "Content Split"
              },
              "media": {
                "jsonValue": {
                  "value": {
                    "src": "https://edge.sitecorecloud.io/zuelligphar9b34-zpcms-test-fcf9/media/Project/ezrx/ezrx/Frame-427321746.jpg?h=824&iar=0&w=584",
